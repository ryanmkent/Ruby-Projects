<table>
	<thead>
		<th>Branch</th>
		<th>City</th>
		<th>Street</th>
		<th>State</th>
	</thead>
	<tbody>	
		<% @dojos.each do |dojo| %>
			<tr>
				<td><%= dojo.branch %></td> 
				<td><%= dojo.city %></td>
				<td><%= dojo.street %></td> 
				<td><%= dojo.state %></td>
				<td><a href="/dojos/<%= dojo.id %>">Show</a></td>
				<td><a href="/edit/<%= dojo.id %>">Edit</a></td>
				<td><a href="/destroy/<%= dojo.id %>" data-method="delete">Delete</a></td>
				Destroy</a></td>
			</tr>	
		<% end %>
	</tbody>	
</table>
<a href='/dojos/new'>New Dojo</a>	