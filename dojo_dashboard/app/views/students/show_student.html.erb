<h1><%= @student.first_name %> from <%= @dojo.city %></h1>
Email: <%= @student.email %>
<h3>Cohort</h3>
<table>
	<thead>
		<th>First Name</th>
		<th>Last Name</th>
		<th>Email</th>
	</thead>
	<tbody>
		<% @cohort.each do |c| %>
			<tr>	
				<td><%= c.first_name %></td>
				<td><%= c.last_name %></td>
				<td><%= c.email %></td>
			</tr>
		<% end %>
	</tbody>
</table>
